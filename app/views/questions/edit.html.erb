<h2>Update question <%= @question.id %></h2>

<%= form_with model: @question do |form| %>
  <p>
    <%= form.label :body, "Question text" %>
  </p>
  <p>
    <%= form.text_area :body, rows: 5 %>
  </p>
  <p>
    <%= form.label :user_id, "User ID" %>
  </p>
  <p>
    <%= form.text_field :user_id %>
  </p>
  <p>
    <%= form.submit  "Save"%>
  </p>
<% end %>